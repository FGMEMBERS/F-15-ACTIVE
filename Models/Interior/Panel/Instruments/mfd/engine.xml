   <animation>
  <type>material</type>
  <object-name>lafterburner</object-name>
 <object-name>rafterburner</object-name>
  <ambient>
    <red>0.0</red>
    <green>0.0</green>
    <blue>0.0</blue>
  </ambient>
  <diffuse>
