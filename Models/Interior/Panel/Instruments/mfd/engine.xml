<?xml version="1.0"?>


<PropertyList>

 
   <animation>
  <type>material</type>
  <object-name>engine-headder</object-name>
   <object-name>L-headder</object-name>
    <object-name>R-headder</object-name>
	<object-name>RPM-headder</object-name>
	<object-name>lrpm</object-name>
	<object-name>rrpm</object-name>
	<object-name>EGT-headder</object-name>
	<object-name>legt</object-name>
	<object-name>regt</object-name>
	<object-name>VECT-headder</object-name>
	<object-name>lvec</object-name>
	<object-name>rvec</object-name>
	<object-name>RHEAT-headder</object-name>
	<object-name>FUEL-headder</object-name>
	<object-name>FUEL</object-name>
  <ambient>
    <red>0.0</red>
    <green>1.0</green>
    <blue>0.0</blue>
  </ambient>
  <diffuse>
    <red>0.0</red>
    <green>0.1</green>
    <blue>0.0</blue>
  </diffuse>
  <emission>
    <red>0.3</red>
    <green>0.8</green>
    <blue>0.3</blue>
  </emission> 
 </animation>
 
 <text>
  <name>engine-headder</name>
  <font>lcd.txf</font>
  <character-size type="double">0.004</character-size>
  <offsets>
   <x-m>0.005</x-m>
   <y-m>-0.045</y-m>
   <z-m>0.053</z-m>
  </offsets>
  <axis-alignment>yz-plane</axis-alignment>
  <type type="string">literal</type>
  <text type="string">ENGINE</text>
  <draw-text type="bool">true</draw-text>
  <alignment>left-top</alignment>
 </text>
 
 
 
 
 
