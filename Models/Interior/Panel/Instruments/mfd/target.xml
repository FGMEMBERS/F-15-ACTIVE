<?xml version="1.0"?>


<PropertyList>

 
   <animation>
  <type>material</type>
  <object-name>Traget-headder</object-name>
    <object-name>Traget-ID</object-name>
	<object-name>ID</object-name>
	<object-name>Traget-Range</object-name>
	<object-name>range</object-name>
	<object-name>Traget-Bearing</object-name>
	<object-name>brng</object-name>
	<object-name>Traget-Alt</object-name>
	<object-name>alt</object-name>
	<object-name>Traget-Speed</object-name>
	<object-name>spd</object-name>
	<object-name>WEAPON-MODE</object-name>
	<object-name>GUN</object-name>
	<object-name>AMMO</object-name>
	<object-name>AA</object-name>
	<object-name>misile</object-name>
  <ambient>
  
    <red>0.0</red>
    <green>1.0</green>
    <blue>0.0</blue>
  </ambient>
  <diffuse>
    <red>0.0</red>
    <green>0.1</green>
    <blue>0.0</blue>
  </diffuse>
  <emission>
    <red>0.3</red>
    <green>0.8</green>
    <blue>0.3</blue>
  </emission> 
 </animation>
 
 <text>
  <name>Traget-headder</name>
  <font>lcd.txf</font>
  <character-size type="double">0.006</character-size>
  <offsets>
   <x-m>0.005</x-m>
   <y-m>-0.045</y-m>
   <z-m>0.053</z-m>
  </offsets>
  <axis-alignment>yz-plane</axis-alignment>
  <type type="string">literal</type>
  <text type="string">TARGET DATA:</text>
  <draw-text type="bool">true</draw-text>
  <alignment>left-top</alignment>
 </text>
 
 
  <text>
  <name>Traget-ID</name>
