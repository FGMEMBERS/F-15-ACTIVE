<!-- animation to display gun/ weapon mode when selected -->

 <animation>
        <type>select</type>
        <object-name>AMMO</object-name>
        <object-name>GUN</object-name>
        <condition>
        <equals> 
        <property>controls/armament/stick-selector</property>
        <value>1</value>
      </equals> 
        </condition>
    </animation>
	
	 <animation>
        <type>select</type>
        <object-name>AA</object-name>
        <condition>
         <equals> 
        <property>controls/armament/stick-selector</property>
        <value>2</value>
      </equals> 
        </condition>
    </animation>
	
	 <animation>
        <type>select</type>
        <object-name>misile</object-name>
        <condition>
         <equals> 
        <property>controls/armament/stick-selector</property>
        <value>2</value>
      </equals> 
	   <input>
      <property>controls/armament/name</property>
    </input>
        </condition>
    </animation>
  
  
